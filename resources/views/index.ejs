    <!-- blog -->
    <section class = "blog container" id = "blog">
      <div class = "container">
        <div class = "title">
          <h2>Latest Blog</h2>
          <p>recent blogs about art & design</p>
        </div>
        <div class = "blog-content">
          <!-- item -->

          <%posts.forEach(function(post){%>
          <div class = "blog-item">
            <div class = "blog-img">
              <img src = "<%=post.thumbnail%>" alt = "">
              <span><i class = "far fa-heart"></i></span>
            </div>
            <div class = "blog-text">
              <span><%=post.published_at%></span>
              <h2><%=post.metadata.heading%></h2>
              <p><%=post.title%></p>
             <button  id="rmbtn"><a href = "/post/<%=post.id%>">Read More</a></button>
            </div>
          </div>
          <%})%>
          <!-- end of item -->
          
        </div>
      </div>
    </section>
    <!-- end of blog -->

